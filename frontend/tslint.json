{
  "extends": ["tslint:latest", "tslint-react"],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts", "**/*.js", "**/*.jsx"]
  },
  "rules": {
    "jsx-no-multiline-js": false,
    "object-literal-sort-keys": false,
    "arrow-parens": false,
    "trailing-comma": false,
    "semicolon": [true, "always", "strict-bound-class-methods"]
  }
}
